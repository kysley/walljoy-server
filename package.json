  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --no-notify src/index.ts",
    "dev:typecheck": "tsc --noEmit --watch",
    "build": "yarn nexus:reflection:temp && tsc",
