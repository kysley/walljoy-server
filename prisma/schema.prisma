datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}

generator db {
  provider = "go run github.com/prisma/prisma-client-go"
}

model Account {
  id      String   @id @default(uuid())
  devices Device[]
}

model Device {
  id         String  @id @default(cuid())
  authorized Boolean
  account    Account @relation(fields: [accountId], references: [id])
  accountId  String
}
